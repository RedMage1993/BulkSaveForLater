{
  "manifest_version": 2,
  "name": "Bulk Save For Later",
  "description": "Bulk save for later on Amazon's shopping cart page.",
  "version": "1.0",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://www.amazon.com/gp/cart/view.html/*"],
      "js": ["js/jquery-3.2.1.min.js", "js/bulkSaveForLater.js"]
    }
  ]
}